{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",

  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.suggest.preview": true,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.enabled": "all",

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tabnine.experimentalAutoImports": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "color-highlight.markerType": "underline",

  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "editor.lineHeight": 1.6,
  "liveServer.settings.CustomBrowser": "chrome",

  "workbench.colorTheme": "Dracula",
  "workbench.colorCustomizations": {
    "[Dracula]": {
      "breadcrumb.background": "#212121",
      "breadcrumb.foreground": "#888",

      "editor.background": "#212121",

      "editorLineNumber.activeForeground": "#448aff",
      "editorLineNumber.foreground": "#777",

      "editor.lineHighlightBackground": "#2c2c2c",
      "editor.lineHighlightBorder": "#2c2c2c",

      "titleBar.activeBackground": "#2c2c2c",
      "titleBar.inactiveBackground": "#2c2c2c",

      "menu.background": "#3e3e3e",
      "menubar.selectionBackground": "#3e3e3e",

      "tab.activeBackground": "#212121",
      "tab.activeForeground": "#448aff",
      "tab.activeBorderTop": "#448aff",
      "tab.inactiveBackground": "#2c2c2c",

      "editorGroupHeader.tabsBackground": "#2c2c2c",

      "sideBar.background": "#2c2c2c",
      "sideBar.border": "#2c2c2c",

      "sideBarSectionHeader.background": "#2c2c2c",
      "sideBarSectionHeader.border": "#212121",

      "activityBar.activeBackground": "#2c2c2c",
      "activityBar.activeBorder": "#448aff",

      "activityBar.foreground": "#448aff",
      "activityBar.background": "#2c2c2c",

      "activityBarBadge.background": "#ff1744",
      "activityBarBadge.foreground": "#fff",

      "activityBar.border": "#2c2c2c",

      "activityBar.inactiveForeground": "#777",
      "list.activeSelectionBackground": "#525252",
      "list.activeSelectionForeground": "#fff",
      "list.focusOutline": "#525252",
      "list.hoverBackground": "#3e3e3e",
      "list.inactiveSelectionBackground": "#3e3e3e",

      "list.activeSelectionIconForeground": "#fff",

      "panel.background": "#212121",

      "panel.border": "#448aff",
      "panelTitle.activeBorder": "#448aff",

      "panelTitle.activeForeground": "#448aff",
      "panelTitle.inactiveForeground": "#aaa",

      "terminal.background": "#2c2c2c",
      "statusBar.background": "#2c2c2c"
    }
  },

  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": "string",
          "settings": {
            "foreground": "#B388FF"
          }
        },
        {
          "scope": "punctuation.definition.string.end",
          "settings": {
            "foreground": "#8c9eff"
          }
        },
        {
          "scope": "punctuation.definition.string.begin",
          "settings": {
            "foreground": "#8c9eff"
          }
        }
      ]
    }
  }
}
